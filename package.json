{
  "name": "tree-sitter-iex",
  "version": "0.0.1",
  "description": "A tree-sitter grammar for Elixir's \"IEx\" REPL syntax",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter test",
    "format": "prettier --trailing-comma es5 --write grammar.js",
    "format-check": "prettier --trailing-comma es5 --check grammar.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-mikedavis/tree-sitter-iex.git"
  },
  "keywords": [
    "parser",
    "lexer",
    "elixir",
    "iex",
    "tree-sitter"
  ],
  "author": "the-mikedavis",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/the-mikedavis/tree-sitter-iex/issues"
  },
  "homepage": "https://github.com/the-mikedavis/tree-sitter-iex#readme",
  "dependencies": {
    "nan": "^2.15.0"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "tree-sitter-cli": "^0.20.1"
  },
  "tree-sitter": [
    {
      "scope": "source.iex",
      "file-types": [
        "iex"
      ],
      "injection-regex": "^iex$"
    }
  ]
}
